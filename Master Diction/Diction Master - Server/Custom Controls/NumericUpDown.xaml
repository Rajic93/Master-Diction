<UserControl x:Class="Diction_Master___Server.Custom_Controls.NumericUpDown"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Diction_Master___Server.Custom_Controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Height="25" Width="100" Loaded="UserControl_Loaded">
    <Grid>
        <Button x:Name="button" Content="&lt;" Width="25" Height="25" Margin="0,0,75,-0.2" Click="button_Click"/>
        <Button x:Name="button_Copy" Content="&gt;" Width="25" Height="25" Margin="75,0,0,-0.2" Click="button_Copy_Click"/>
        <TextBox x:Name="textBox" Height="23" Margin="25,1,25,0.8" TextWrapping="Wrap" Width="50" TextChanged="textBox_TextChanged"/>

    </Grid>
</UserControl>
