<UserControl x:Class="Diction_Master.UserControls.ChoiceQuestion"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Diction_Master.UserControls"
             mc:Ignorable="d" 
             MinHeight="300" MinWidth="300" Background="{DynamicResource {x:Static SystemColors.ControlLightLightBrushKey}}">
    <Grid>
        <Button x:Name="button" Content="Next" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="75"/>
        <TextBlock x:Name="textBlock" Margin="10,10,10,175" TextWrapping="Wrap" Text="TextBlock"/>
        <StackPanel x:Name="StackPanel" Margin="10,130,10,34" />
    </Grid>
</UserControl>
